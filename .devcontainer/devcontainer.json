{
	"name": "Go",
	"dockerFile": "Dockerfile",
	"appPort": 9000,
	"extensions": [
		"ms-vscode.go"
	],
	"settings": {
		"go.gopath": "/go",
		"go.inferGopath": true,
		"go.useLanguageServer": true,
		"[go]": {
				"editor.formatOnSave": true,
				"editor.codeActionsOnSave": {
						"source.organizeImports": true
				}
		},
		"gopls": {
				"usePlaceholders": true
		}
	},
	"postCreateCommand": "make deps"
}
